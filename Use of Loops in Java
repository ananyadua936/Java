public class Loops {
    public static void main(String[] args){
    //FOR LOOP
    for(int i = 0; i < 11; i++){
    System.out.print(i);
 }
 //While LOOP
        int i =0;
     while(i<11){
     System.out.println(i);
   i= i+1;
    }
    //DO WHILE LOOP
    int i=0;
     do{ 
     System.out.println(i);
     }
    while(i<11);
 
    }
}
